<p>
<div class="container">
  <% @users.each do |user| %>
    <h3 style="margin: -4px;"><%= "#{user.first_name} #{user.last_name}" %></h3>
    <p>email: <%= user.email %></p>
    <p>User ID: <%= user.id %></p>

    <%= link_to "New User ", new_user_path(@user) %>
    <%= link_to "Edit User ", edit_user_path(user) %>
    <%= link_to  "Delete", user_path(user), method: :delete %>
    </br></br>
  <% end %>
</div>