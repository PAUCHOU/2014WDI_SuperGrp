<p>
<div class="container">
<% @users.each do |user| %>

  <%= form_for user do |f| %>


      <h3 style="margin: -3px;"><%= f.label :name, "#{user.first_name} #{user.last_name}" %></h3>
      <%= f.label :email, "#{user.email}" %></br>

      <%= link_to "New User ", new_user_path(@user) %></br>
      <%= link_to "Edit User ", edit_user_path(user) %></br>
      <%= link_to  "Delete", user_path(user), method: :delete %>

  <% end %>
<% end %>

</div>
